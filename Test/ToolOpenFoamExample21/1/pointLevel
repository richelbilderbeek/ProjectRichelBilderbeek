FoamFile
{
    version     2.0;
    format      ascii;
    class       pointScalarField;
    location    "1";
    object      pointLevel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

