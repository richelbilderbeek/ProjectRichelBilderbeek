{
    my_cube_faces
    {
        type            calculated;
    }
    outlet
    {
        type            calculated;
    }
    defaultName
    {
        type            calculated;
    }
    inlet
    {
        type            calculated;
    }
    outlet_patch0
    {
        type            calculated;
    }
    defaultName_patch0
    {
        type            calculated;
    }
    inlet_patch0
    {
        type            calculated;
    }
}


